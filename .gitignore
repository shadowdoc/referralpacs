# Ignore bundler config
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3

# Ignore all logfiles and tempfiles.
/log
/log/*.log
/tmp

# Ignore old IDE files
/.idea

# Ignore config files
/config/database.yml
/config/openmrs.yml
/config/dcm4chee.yml
/config/email.yml

# Ignore capistrano config
/config/deploy.rb

# Ignore anything in the image archive
/image_archive

# Ignore any HL7 messages
/hl7

# Ignore Capistrano
Capfile

# Ignore MacOS files
.DS_Store